{
  "name": "kifaru-frontend",
  "version": "1.0.0",
  "description": "errorSwag frontend",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint \"**/*.{js,json}\" --write",
    "prettier": "prettier **/*.{js,json} --write",
    "format": "pretty-quick",
    "develop": "webpack --env.mode development --watch",
    "heroku-postbuild": "npm run build",
    "start": "node server.js",
    "build": "webpack --env.mode production",
    "start:dev": "webpack-dev-server --hot --env.mode development --open",
    "cypress": "cypress open",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run prettier",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/ah-kifaru-frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/ah-kifaru-frontend/issues"
  },
  "homepage": "https://github.com/andela/ah-kifaru-frontend#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "coveralls": "^3.0.5",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^1.11.1",
    "purgecss-webpack-plugin": "^1.5.0",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.0.6",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  }
}
